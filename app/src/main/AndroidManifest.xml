<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devteamlobach.chatgpt" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_chatgpt_assistant"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_chatgpt_assistant"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatGPT"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.devteamlobach.chatgpt.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>